<%= create_date_range('1810-1819', base_url, @selection) %> •
<%= create_date_range('1820-1829', base_url, @selection) %> •
<%= create_date_range('1830-1833', base_url, @selection) %>
</div>
<%= render :partial => 'three_column_list', :locals => { :list_item => @list, :action => { :controller => 'materials', :action => 'show' }, :label_column => :id, :table_class => "student_browse_list" } %>
